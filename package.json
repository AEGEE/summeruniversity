{
  "name": "summeruniversity",
  "version": "1.4.17",
  "description": "Summer University module of MyAEGEE",
  "main": "lib/server.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "NODE_ENV=test npm run db:setup && jest test/api/*.js --runInBand --forceExit",
    "test:ci": "NODE_ENV=test npm run db:setup && jest --runInBand --forceExit",
    "lint": "node_modules/.bin/eslint .",
    "lint-staged": "lint-staged",
    "db:create": "sequelize db:create",
    "db:setup": "sequelize db:drop; sequelize db:create; sequelize db:migrate",
    "db:migrate": "sequelize db:migrate",
    "cli": "node --experimental-repl-await -r ./cli.js",
    "open-coverage": "open-cli coverage/lcov-report/index.html",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**/*.js",
      "models/**/*.js",
      "!lib/run.js",
      "!lib/sequelize.js",
      "!lib/logger.js"
    ]
  },
  "author": "Rik Smale <github.com/WikiRik>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AEGEE/summeruniversity/issues"
  },
  "homepage": "https://github.com/AEGEE/summeruniversity#readme",
  "dependencies": {
    "@bugsnag/js": "^7.22.3",
    "@faker-js/faker": "^8.3.1",
    "body-parser": "^1.20.2",
    "bunyan": "^1.8.15",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-promise-router": "^4.1.1",
    "express-query-boolean": "^2.0.0",
    "file-type": "^16.5.4",
    "joi": "^17.12.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "node-schedule": "^2.1.1",
    "node-xlsx": "^0.23.0",
    "on-finished": "^2.4.1",
    "pg": "^8.11.3",
    "prom-client": "^15.1.0",
    "read-chunk": "^3.2.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "rimraf": "^5.0.5",
    "sequelize": "^6.35.2",
    "sequelize-cli": "^6.6.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^18.4.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "commitlint": "^18.4.4",
    "conventional-changelog": "^5.1.0",
    "conventional-changelog-cli": "^4.1.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^15.2.1",
    "nock": "^13.4.0",
    "nyc": "^15.1.0",
    "open-cli": "^8.0.0",
    "semantic-release": "^22.0.12",
    "timekeeper": "^2.3.1"
  }
}
