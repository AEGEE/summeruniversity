{
  "name": "summeruniversity",
  "version": "1.1.0",
  "description": "Summer University module of MyAEGEE",
  "main": "lib/server.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "NODE_ENV=test npm run db:setup && jest test/api/*.js --runInBand --forceExit",
    "lint": "node_modules/.bin/eslint .",
    "db:create": "sequelize db:create",
    "db:setup": "sequelize db:drop; sequelize db:create; sequelize db:migrate",
    "db:migrate": "sequelize db:migrate",
    "open-coverage": "open-cli coverage/lcov-report/index.html",
    "cli": "node --experimental-repl-await -r ./cli.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**/*.js",
      "models/**/*.js",
      "!lib/run.js",
      "!lib/sequelize.js",
      "!lib/logger.js"
    ]
  },
  "author": "Rik Smale <github.com/WikiRik>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AEGEE/summeruniversity/issues"
  },
  "homepage": "https://github.com/AEGEE/summeruniversity#readme",
  "dependencies": {
    "@bugsnag/js": "^7.7.0",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.15",
    "express": "^4.17.1",
    "express-promise-router": "^4.0.1",
    "express-query-boolean": "^2.0.0",
    "faker": "^5.4.0",
    "file-type": "^16.2.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "node-schedule": "^2.0.0",
    "node-xlsx": "^0.16.1",
    "on-finished": "^2.3.0",
    "pg": "^8.5.1",
    "prom-client": "^13.1.0",
    "read-chunk": "^3.2.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "rimraf": "^3.0.2",
    "sequelize": "^6.5.0",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "codecov": "^3.8.1",
    "commitlint": "^11.0.0",
    "conventional-changelog": "^3.1.24",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "lint-staged": "^10.5.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nock": "^13.0.7",
    "nyc": "^15.1.0",
    "open-cli": "^6.0.1",
    "semantic-release": "^17.3.9",
    "timekeeper": "^2.2.0"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/github",
      "@semantic-release/git"
    ],
    "tagFormat": "${version}"
  }
}
